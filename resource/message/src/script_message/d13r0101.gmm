	<row id="msg_d13r0101_assistm_01" back-color="#8080ff,,,#8080ff,,#8080ff,#8080ff,#8080ff,,,#8080ff,,#8080ff,#8080ff,,,#8080ff,,">
		<comment>●研究員のポジション（GRIDX=4/GRIDZ=3）
