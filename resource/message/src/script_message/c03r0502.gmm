	<row id="msg_c03r0502_mrlock_01" text-color=",,,#000000,,,,,,,,,,,,,,," back-color="#ff78ff,,,#ff78ff,,#ff78ff,#ff78ff,,,,,,#ff78ff,,,,,,">
