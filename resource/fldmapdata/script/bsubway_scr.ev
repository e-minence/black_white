sub_receipt_anm_partner_in:
{
  DEFINE_LOCAL objid
  
  //OBJ ID取得
  _CALL sub_receipt_common_get_objid_partner
  $objid = $SCWK_ANSWER
  
  //サポート追加
  _CALL sub_receipt_common_add_partner
  
  //場所変更
  _OBJ_POS_CHANGE( objid, 2, 1, 12, DIR_RIGHT )
  
  //自機の前まで歩く
  _OBJ_ANIME( objid, anm_receipt_partner_in_partner )
  _OBJ_ANIME_WAIT()
  
  //自機振り向き
  _CALL sub_receipt_anm_jiki_turn_partner
  
  //初遭遇か？
  _BSUBWAY_TOOL( BSWTOOL_GET_SUPPORT_ENCOUNT_END, BSW_NULL, BSW_NULL, SCWK_ANSWER )
  
  IF $SCWK_ANSWER == 0 THEN
    $SCWK_TEMP0 = msg_bsw_scr_57
    $SCWK_TEMP1 = msg_bsw_scr_58
    _CALL sub_partner_common_bwin

    _BSUBWAY_TOOL( BSWTOOL_SET_SUPPORT_ENCOUNT_END, BSW_NULL, BSW_NULL, BSW_NULL )
  ELSE
    $SCWK_TEMP0 = msg_bsw_scr_59
    $SCWK_TEMP1 = msg_bsw_scr_60
    _CALL sub_partner_common_bwin
  ENDIF
  
  _AB_KEYWAIT()
  _MSGWIN_CLOSE()
}
_RET

