	IF $SCWK_TEMP1 == 0 THEN
		_CALL	ev_bigfour_menu	//四天王部屋

	ELSIF $SCWK_TEMP1 == 1 THEN
		//テスト01
//		_FLAG_RESET( FV_FLDMONS_06 )
//		_MAP_CHANGE( ZONE_ID_R10, 431, 114, DIR_LEFT )

