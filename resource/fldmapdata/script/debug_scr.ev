		//ワークを２に
		$WK_SCENE_N01R0502 = 2
		
		_MAP_CHANGE( ZONE_ID_N01R0502, 16, 45, DIR_UP )

	ELSIF $LWK_EVTJUMP == 100 THEN
		//ラストシーン・Ｎ戦前
		//OBJ調整
		_FLAG_RESET( FV_N01R0502_LEGEND_01 )	//Nの伝説
		_FLAG_SET( FV_N01R0502_LEGEND_02 )	//自機の伝説
		_FLAG_SET( FV_N01R0502_EVENT_01 )

		//謁見の間、水が干上がった状態のマップに変更
		_CHANGE_MAPREPLACE_FLAG( MAPREPLACE_ID_04, TRUE, TRUE )

		//ワークを３に
		$WK_SCENE_N01R0502 = 3
		
		_MAP_CHANGE( ZONE_ID_N01R0502, 16, 45, DIR_UP )

	ELSIF $LWK_EVTJUMP == 110 THEN
		//ラストシーン・ゲーチス戦前
		//OBJセット
		_FLAG_SET( FV_C09R0501_RIVAL_01 )
		_FLAG_SET( FV_C09R0501_CHAMPION_01 )
		_FLAG_SET( FV_N01R0501_SAGE1 )
		_FLAG_RESET( FV_N01R0502_EVENT_01 )

		//謁見の間、水が干上がった状態のマップに変更
		_CHANGE_MAPREPLACE_FLAG( MAPREPLACE_ID_04, TRUE, TRUE )

		//ワーク
		$WK_SCENE_N01R0502 = 5
		$WK_SCENE_C09R0501 = 2	
			
		_MAP_CHANGE( ZONE_ID_N01R0502, 16, 45, DIR_UP )
		
	ENDIF
	}
	
_RET


