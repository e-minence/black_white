		$WK_SCENE_R07_MOVEPOKE = 1
		//R07の天気を変更
		_FLAG_SET( SYS_FLAG_WEATHER_R07 )

		//最初の一歩イベント
//		$WK_SCENE_T01 = 2
//		_MAP_CHANGE( ZONE_ID_T01, 787, 742, DIR_DOWN )

	ELSIF $SCWK_TEMP1 == 2 THEN
		//テスト02

		$WK_SYS_FIRST_POKETYPE = FIRST_POKETYPE_MIZU

		//ママ：タウンマップ入手
/*		_FLAG_SET( FV_T01_RIVAL )
		_FLAG_SET( FV_T01_SUPPORT )
		_FLAG_RESET( FV_T01_MAMA_01 )
		$WK_SCENE_T01 = 1
		$WK_SCENE_T01R0401 = 1
		_MAP_CHANGE( ZONE_ID_T01R0401, 4, 10, DIR_DOWN )
*/

	ELSIF $SCWK_TEMP1 == 3 THEN
		//テスト03
		_FLAG_RESET( FV_C09R0701_WARPMAN )

		_SUBWIN_MSG( msg_debug_masafumi_15, 10, 10, 1 )
		_AB_KEYWAIT()
		_SUBWIN_CLOSE( 1 )

//クリア後ダークトリニティ
//		$WK_SCENE_H05 = 0
//		_MAP_CHANGE( ZONE_ID_H05, 37, 33, DIR_DOWN )
		
	ELSIF $SCWK_TEMP1 == 4 THEN
		//観覧車トレーナー
		_FLAG_RESET( TMFLG_AMUSEMENT_TR_BTL )
		_FLAG_RESET( FV_AMUSEMENT_TRAINER )
		_MAP_CHANGE( ZONE_ID_C04P01, 52, 15, DIR_RIGHT )
 
	ELSIF $SCWK_TEMP1 == 5 THEN
		//カナワタウンの転車台に配置される車両を変更
		_BMPMENU_INIT_EX( 1, 1, 0, 1, SCWK_ANSWER )
		_BMPMENU_MAKE_LIST( msg_debug_masafumi_14_01, 1 )	//シングル
		_BMPMENU_MAKE_LIST( msg_debug_masafumi_14_02, 2 )	//Ｓシングル
		_BMPMENU_MAKE_LIST( msg_debug_masafumi_14_03, 3 )	//ダブル
		_BMPMENU_MAKE_LIST( msg_debug_masafumi_14_04, 4 )	//Ｓダブル
		_BMPMENU_MAKE_LIST( msg_debug_masafumi_14_05, 5 )	//マルチ
		_BMPMENU_MAKE_LIST( msg_debug_masafumi_14_06, 6 )	//Ｓマルチ
		_BMPMENU_MAKE_LIST( msg_debug_masafumi_14_07, 7 )	//Wi-Fi
		_BMPMENU_MAKE_LIST( msg_debug_masafumi_14_08, 8 )	//カナワ行き
		_BMPMENU_MAKE_LIST( msg_debug_masafumi_14_09, 9 )	//古いレア
		_BMPMENU_MAKE_LIST( msg_debug_masafumi_14_10, 10 )	//新しいレア
		_BMPMENU_MAKE_LIST( msg_debug_01, 0 )	//テスト01（車両なし）
		_BMPMENU_MAKE_LIST( msg_debug_00, 100 )	//やめる
		_BMPMENU_START()
		
		SWITCH $SCWK_ANSWER
		CASE 0 THEN
			$WK_OTHER_T05_TRAIN = 0
		CASE 1 THEN
			$WK_OTHER_T05_TRAIN = 1
		CASE 2 THEN
			$WK_OTHER_T05_TRAIN = 2
		CASE 3 THEN
			$WK_OTHER_T05_TRAIN = 3
		CASE 4 THEN
			$WK_OTHER_T05_TRAIN = 4
		CASE 5 THEN
			$WK_OTHER_T05_TRAIN = 5
		CASE 6 THEN
			$WK_OTHER_T05_TRAIN = 6
		CASE 7 THEN
			$WK_OTHER_T05_TRAIN = 7
		CASE 8 THEN
			$WK_OTHER_T05_TRAIN = 8
		CASE 9 THEN
			$WK_OTHER_T05_TRAIN = 9
		CASE 10 THEN
			$WK_OTHER_T05_TRAIN = 10
		ENDSWITCH
		
		IF $SCWK_ANSWER <= 10 THEN
			_MAP_CHANGE( ZONE_ID_T05, 25, 38, DIR_UP )
		ENDIF
		
	ENDIF
	
EVENT_END


//四天王部屋デバッグメニュー------------------------------------
ev_bigfour_menu:
	//メニュー作成
	_BMPMENU_INIT_EX( 1, 1, 0, 1, SCWK_ANSWER )
	//リスト追加
	_BMPMENU_MAKE_LIST( msg_debug_masafumi_07, 0 )	//勝利フラグ
	_BMPMENU_MAKE_LIST( msg_debug_04, 1 )	//ジャンプ ※階段前にジャンプ（アニメチェック用）
	_BMPMENU_MAKE_LIST( msg_debug_00, 10 )	//やめる
	//メニュー表示
	_BMPMENU_START()

	IF $SCWK_ANSWER == 0 THEN
		
		WHILE $SCWK_TEMP0 != 111
		
			_BMPMENU_INIT_EX( 1, 1, 0, 1, SCWK_PARAM0 )
			_BMPMENU_MAKE_LIST( msg_debug_masafumi_01, 0 )	//ゴースト
			_BMPMENU_MAKE_LIST( msg_debug_masafumi_02, 1 )	//あく
			_BMPMENU_MAKE_LIST( msg_debug_masafumi_03, 2 )	//かくとう
			_BMPMENU_MAKE_LIST( msg_debug_masafumi_04, 3 )	//エスパー
			_BMPMENU_MAKE_LIST( msg_debug_00, 10 )	//やめる
			_BMPMENU_START()
			
			IF $SCWK_PARAM0 == 0 THEN
				_FLAG_SET( SYS_FLAG_BIGFOUR_GHOSTWIN )
			ELSIF $SCWK_PARAM0 == 1 THEN
				_FLAG_SET( SYS_FLAG_BIGFOUR_EVILWIN )
			ELSIF $SCWK_PARAM0 == 2 THEN
				_FLAG_SET( SYS_FLAG_BIGFOUR_FIGHTWIN )
			ELSIF $SCWK_PARAM0 == 3 THEN
				_FLAG_SET( SYS_FLAG_BIGFOUR_ESPWIN )
			ELSE
				$SCWK_TEMP0 = 111
			ENDIF
		
		ENDWHILE
		
	ELSIF $SCWK_ANSWER == 1 THEN
		
		
		_BMPMENU_INIT_EX( 1, 1, 0, 1, SCWK_PARAM0 )
		_BMPMENU_MAKE_LIST( msg_debug_masafumi_01, 0 )	//ゴースト
		_BMPMENU_MAKE_LIST( msg_debug_masafumi_02, 1 )	//あく
		_BMPMENU_MAKE_LIST( msg_debug_masafumi_03, 2 )	//かくとう
		_BMPMENU_MAKE_LIST( msg_debug_masafumi_04, 3 )	//エスパー
		_BMPMENU_START()
		
		IF $SCWK_PARAM0 == 0 THEN
			_MAP_CHANGE( ZONE_ID_C09R0101, 15, 28, DIR_DOWN )
		ELSIF $SCWK_PARAM0 == 1 THEN
			_MAP_CHANGE( ZONE_ID_C09R0201, 15, 28, DIR_DOWN )
		ELSIF $SCWK_PARAM0 == 2 THEN
			_MAP_CHANGE( ZONE_ID_C09R0301, 15, 28, DIR_DOWN )
		ELSIF $SCWK_PARAM0 == 3 THEN
			_MAP_CHANGE( ZONE_ID_C09R0401, 15, 28, DIR_DOWN )
		ENDIF
		
		
	ENDIF

_RET


