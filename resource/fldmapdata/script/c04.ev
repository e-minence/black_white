/************************************************************/
/*					プラズマ団 posイベント					*/
/************************************************************/
EVENT_START	ev_pos_c04_hakai_01
	
	{
	
	DEFINE_LOCAL	LWK_POSX
	DEFINE_LOCAL	LWK_POSZ
	
	_PLAYER_POS_GET( LWK_POSX, LWK_POSZ )
	//主人公：前へ
	IF $LWK_POSX == 427 THEN
		_OBJ_ANIME( MMDL_ID_PLAYER, anm_hakai_player_move_01a )
	ELSIF $LWK_POSX == 428 THEN
		_OBJ_ANIME( MMDL_ID_PLAYER, anm_hakai_player_move_01b )
	ELSIF $LWK_POSX == 429 THEN
		_OBJ_ANIME( MMDL_ID_PLAYER, anm_hakai_player_move_01c )
	ELSIF $LWK_POSX == 430 THEN
		_OBJ_ANIME( MMDL_ID_PLAYER, anm_hakai_player_move_01d )
	ELSIF $LWK_POSX == 431 THEN
		_OBJ_ANIME( MMDL_ID_PLAYER, anm_hakai_player_move_01e )
	ELSIF $LWK_POSX == 432 THEN
		_OBJ_ANIME( MMDL_ID_PLAYER, anm_hakai_player_move_01f )
	ELSE
		_OBJ_ANIME( MMDL_ID_PLAYER, anm_hakai_player_move_01g )
	ENDIF
	_OBJ_ANIME_WAIT()
	
	}
	
	_CAMERA_START()		//カメラ操作開始
	_CAMERA_PURGE()		//対象物とカメラのバインドを解く
	
	//カメラ操作
	_CAMERA_MOVE( 9688,0,970752,28213248,65551,30564352,30 )	//一番最後の引数は移動にかかるフレーム数
	_CAMERA_WAIT_MOVE()	//カメラ移動待ち
	
	//じいさん！あんたが育て屋…
	_BALLOONWIN_OBJMSG_POS( msg_c04_hakaim1_01, C04_HAKAIM_01, WIN_NONE )
	_MSGWIN_CLOSE()
	
	//オレたちはポケモンを
	_BALLOONWIN_OBJMSG_POS( msg_c04_hakaim2_01, C04_HAKAIM_02, WIN_NONE )
	_MSGWIN_CLOSE()
	
	//なんというムチャを！
	_BALLOONWIN_OBJMSG_POS( msg_c04_oldman_01, C04_OLDMAN1_01, WIN_NONE )
	_MSGWIN_CLOSE()
	
	//じいさん：！マーク
	_OBJ_ANIME( C04_OLDMAN1_01, anm_sodateya_gyoe_01 )
	_OBJ_ANIME_WAIT()
	
	//主人公がいる位置にカメラを戻す
	_CAMERA_DEF_MOVE( 16 )
	
	//じいさん：主人公横へ
	_OBJ_ANIME( C04_OLDMAN1_01, anm_sodateya_come_01 )
	//したっぱズ：主人公を見る
	_OBJ_ANIME( C04_HAKAIM_01, anm_stay_walk_d )
	_TIME_WAIT( 4 )
	_OBJ_ANIME( C04_HAKAIM_02, anm_stay_walk_d )
	_OBJ_ANIME_WAIT()
	
	_CAMERA_WAIT_MOVE()	//カメラ移動待ち
	_CAMERA_BIND()		//カメラバインド
	_CAMERA_END()		//カメラ操作終了
	
	//おお！トレーナーさん
	_BALLOONWIN_OBJMSG_POS( msg_c04_oldman_02, C04_OLDMAN1_01, WIN_NONE )
	_MSGWIN_CLOSE()
	
	//したっぱ：主人公前へ
	_OBJ_ANIME( C04_HAKAIM_01, anm_hakai_move_01a )
	_OBJ_ANIME( C04_HAKAIM_02, anm_hakai_move_01b )
	_OBJ_ANIME_WAIT()
	
	//だれだろうが…
	_BALLOONWIN_OBJMSG_POS( msg_c04_hakaim1_02, C04_HAKAIM_02, WIN_NONE )
	_MSGWIN_CLOSE()
	
	//トレーナー戦呼び出し
	_TRAINER_BTL_SET( TRID_HAKAIM1_11, 0 )
	//敗戦チェック
	_TRAINER_LOSE_CHECK( SCWK_ANSWER )
	IF $SCWK_ANSWER == SCR_BATTLE_RESULT_WIN THEN
		_TRAINER_WIN()			//勝ち(1)ならトレーナー勝利処理へ
	ELSE
		_TRAINER_LOSE()			//負け(0)ならトレーナー敗北処理へ(ここでスクリプト終了します）
	ENDIF
	
	//！
	_BALLOONWIN_OBJMSG_POS( msg_c04_hakaim1_03, C04_HAKAIM_02, WIN_NONE )
	_MSGWIN_CLOSE()
	
	//なんだコイツ！
	_BALLOONWIN_OBJMSG_POS( msg_c04_hakaim2_02, C04_HAKAIM_01, WIN_NONE )
	_MSGWIN_CLOSE()
	
	//したっぱ：去る
	_OBJ_ANIME( C04_HAKAIM_01, anm_hakai_goaway_01a )
	_OBJ_ANIME( C04_HAKAIM_02, anm_hakai_goaway_01b )
	_OBJ_ANIME_WAIT()
	
	//したっぱ消す
	_OBJ_DEL( C04_HAKAIM_01 )
	_OBJ_DEL( C04_HAKAIM_02 )
	
	//主人公：じいさんを見る
	_OBJ_ANIME( MMDL_ID_PLAYER, anm_stay_walk_l )
	_OBJ_ANIME_WAIT()
	
	//ありがとうよ！
	_BALLOONWIN_OBJMSG_POS( msg_c04_oldman_03, C04_OLDMAN1_01, WIN_NONE )
	_MSGWIN_CLOSE()
	
	//わざマシン「かいりき」入手
	_ITEM_EVENT_KEYWAIT( ITEM_WAZAMASIN95, 1 )
	
	//わしは３番道路で
	_BALLOONWIN_OBJMSG_POS( msg_c04_oldman_04, C04_OLDMAN1_01, WIN_NONE )
	_MSGWIN_CLOSE()
	
	//じいさん：去る
	_OBJ_ANIME( C04_OLDMAN1_01, anm_sodateya_goaway_01 )
	_TIME_WAIT( 8 )
	//主人公：見送る
	_OBJ_ANIME( MMDL_ID_PLAYER, anm_stay_walk_d )
	_OBJ_ANIME_WAIT()
	
	//じいさん消す
	_SE_PLAY( SEQ_SE_KAIDAN )
	_OBJ_DEL( C04_OLDMAN1_01 )
	_SE_WAIT()
	
	
	//サポート表示
	_OBJ_ADD_EX( 431, 477, DIR_UP, MMDL_ID_EVENTOBJ_00, SUPPORT, MV_DMY )
	
	//おーい！
	_PLAYER_NAME( 0 )
	_PLAINWIN_MSG_DOWN( msg_c04_white_01 )
	_MSGWIN_CLOSE()
	
	//サポート曲再生
	_START_EVENT_BGM( SEQ_BGM_E_SUPPORT )
	
	//サポート：主人公横へ
	_OBJ_ANIME( MMDL_ID_EVENTOBJ_00, anm_support_come_01 )
	_TIME_WAIT( 50 )
	_OBJ_ANIME( MMDL_ID_PLAYER, anm_stay_walk_r )
	_OBJ_ANIME_WAIT()
	
	//ライモンシティって見所いっぱいでしょ
	_BALLOONWIN_OBJMSG_POS( msg_c04_white_02, MMDL_ID_EVENTOBJ_00, WIN_NONE )
	_MSGWIN_CLOSE()
	
	//サポート：くるっと
	_OBJ_ANIME( MMDL_ID_EVENTOBJ_00, anm_support_dance_01 )
	_OBJ_ANIME_WAIT()
	
	//あたしはミュージカルにいくつもり じゃあね
	_BALLOONWIN_OBJMSG_POS( msg_c04_white_03, MMDL_ID_EVENTOBJ_00, WIN_NONE )
	_MSGWIN_CLOSE()
	
	//サポート：去る
	_OBJ_ANIME( MMDL_ID_EVENTOBJ_00, anm_support_goaway_01 )
	_OBJ_ANIME_WAIT()
	
	//サポート消す
	_OBJ_DEL( MMDL_ID_EVENTOBJ_00 )
	//サポート曲停止
	_END_EVENT_BGM()
	
	//========================================//
	
	//POSイベント無効
	$WK_SCENE_C04 = 1
	
	//したっぱ＆育て屋じいさんを非表示
	_FLAG_SET( FV_C04_HAKAI_01 )
	
	//３番道路に育て屋じいさんを表示
	_FLAG_RESET( FV_R03_SODATEYA_01 )
	
	//育て屋に２匹預けられるようにする
	_FLAG_SET( FE_SODATEYAOLDMAN_OPEN )
	
	//========================================//
	
EVENT_END


