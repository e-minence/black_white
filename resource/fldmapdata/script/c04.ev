EVENT_START	ev_scene_c04_support_01
	
	//主人公：サポートの近くへ
	_OBJ_ANIME( MMDL_ID_PLAYER, anm_papa_player_move_01 )
	_OBJ_ANIME_WAIT()
	
	//パパ！？どうしてここに
	_BALLOONWIN_OBJMSG_POS( msg_c04_white_08, C04_SUPPORT_01, WIN_NONE )
	_MSGWIN_CLOSE()
	
	//どうもこうも
	_BALLOONWIN_OBJMSG_POS( msg_c04_middleman1_01, C04_SUPPORTPAPA_01, WIN_NONE )
	_MSGWIN_CLOSE()
	
	//サポート：後ずさり
	_OBJ_ANIME( C04_SUPPORT_01, anm_papa_support_back_01 )
	_OBJ_ANIME_WAIT()
	
	//やだぁ！
	_PLAYER_NAME( 0 )
	_BALLOONWIN_OBJMSG_POS( msg_c04_white_09, C04_SUPPORT_01, WIN_NONE )
	_MSGWIN_CLOSE()
	
	//パパ：一歩前へ
	_OBJ_ANIME( C04_SUPPORTPAPA_01, anm_papa_papa_move_01 )
	_OBJ_ANIME_WAIT()
	
	//ダメだ！よそはよそウチはウチだ
	_BALLOONWIN_OBJMSG_POS( msg_c04_middleman1_02, C04_SUPPORTPAPA_01, WIN_NONE )
	_MSGWIN_CLOSE()
	
	//サポート：パパを見て
	_OBJ_ANIME( C04_SUPPORT_01, anm_stay_walk_l )
	_OBJ_ANIME_WAIT()
	
	//だったら！
	_BALLOONWIN_OBJMSG_POS( msg_c04_white_10, C04_SUPPORT_01, WIN_NONE )
	_MSGWIN_CLOSE()
	
	_TIME_WAIT( 8 )
	
	//おじょうさん…
	_PLAINWIN_MSG_DOWN( msg_c04_leader_01 )
	_MSGWIN_CLOSE()
	
	//サポート＆パパ：？マーク
	_OBJ_ANIME( C04_SUPPORT_01, anm_mark_hatena )
	_TIME_WAIT( 8 )
	_OBJ_ANIME( C04_SUPPORTPAPA_01, anm_mark_hatena )
	_OBJ_ANIME_WAIT()
	
	//サポート＆パパ：下を見る
	_OBJ_ANIME( C04_SUPPORT_01, anm_stay_walk_d )
	_OBJ_ANIME( C04_SUPPORTPAPA_01, anm_stay_walk_d )
	
	_CAMERA_START()		//カメラ操作開始
	_CAMERA_PURGE()		//カメラパージ
	_CAMERA_MOVE( 10488,0,970752,28409856,65551,28672000, 20 )
	
	//カミツレ：登場
	_OBJ_ADD_EX( 433, 444, DIR_UP, MMDL_ID_EVENTOBJ_00, LEADER4, MV_DMY )
	_OBJ_ANIME( MMDL_ID_EVENTOBJ_00, anm_papa_leader4_come_01 )
	_OBJ_ANIME_WAIT()
	
	_CAMERA_WAIT_MOVE()	//カメラ移動待ち
	
	//ちょっと！
	_BALLOONWIN_OBJMSG_POS( msg_c04_middleman1_03, C04_SUPPORTPAPA_01, WIN_UP )
	_MSGWIN_CLOSE()
	
	//カミツレと申します
	_BALLOONWIN_OBJMSG_POS( msg_c04_leader_02, MMDL_ID_EVENTOBJ_00, WIN_NONE )
	_MSGWIN_CLOSE()
	
	//カミツレ：サポートの隣へいどう
	_OBJ_ANIME( MMDL_ID_EVENTOBJ_00, anm_papa_leader4_move_01 )
	_OBJ_ANIME_WAIT()
	//パパ：カミツレを見る
	_OBJ_ANIME( C04_SUPPORTPAPA_01, anm_stay_walk_r )
	_OBJ_ANIME_WAIT()
	
	//もちろん…
	_BALLOONWIN_OBJMSG_POS( msg_c04_leader_03, MMDL_ID_EVENTOBJ_00, WIN_NONE )
	_MSGWIN_CLOSE()
	
	//そうでしょうとも！
	_BALLOONWIN_OBJMSG_POS( msg_c04_middleman1_04, C04_SUPPORTPAPA_01, WIN_UP )
	_MSGWIN_CLOSE()
	
	//カミツレ：父を見て
	_OBJ_ANIME( MMDL_ID_EVENTOBJ_00, anm_stay_walk_l )
	_OBJ_ANIME_WAIT()
	
	//ですが…
	_BALLOONWIN_OBJMSG_POS( msg_c04_leader_04, MMDL_ID_EVENTOBJ_00, WIN_NONE )
	_MSGWIN_CLOSE()
	
	//サポート：一歩前へ出て
	_OBJ_ANIME( C04_SUPPORT_01, anm_papa_support_move_01 )
	_OBJ_ANIME_WAIT()
	
	//おねがい！
	_BALLOONWIN_OBJMSG_POS( msg_c04_white_11, C04_SUPPORT_01, WIN_UP )
	_MSGWIN_CLOSE()
	
	//パパ：下を向いて
	_OBJ_ANIME( C04_SUPPORTPAPA_01, anm_stay_walk_d )
	_OBJ_ANIME_WAIT()
	
	//……むむっ
	_BALLOONWIN_OBJMSG_POS( msg_c04_middleman1_05, C04_SUPPORTPAPA_01, WIN_UP )
	_MSGWIN_CLOSE()
	
	//パパ：カミツレを見て
	_OBJ_ANIME( C04_SUPPORTPAPA_01, anm_stay_walk_r )
	_OBJ_ANIME_WAIT()
	
	//ジムリーダーさんに言われて気づいたよ
	_PLAYER_NAME( 0 )
	_BALLOONWIN_OBJMSG_POS( msg_c04_middleman1_06, C04_SUPPORTPAPA_01, WIN_UP )
	_MSGWIN_CLOSE()
	
	//パパ：去る
	_OBJ_ANIME( C04_SUPPORTPAPA_01, anm_papa_papa_goaway_01 )
	_TIME_WAIT( 16 )
	//サポート＆カミツレ：去る父を見送る
	_OBJ_ANIME( MMDL_ID_EVENTOBJ_00, anm_stay_walk_d )
	_OBJ_ANIME( C04_SUPPORT_01, anm_stay_walk_d )
	_OBJ_ANIME_WAIT()
	_OBJ_DEL( C04_SUPPORTPAPA_01 )
	
	_CAMERA_MOVE( 9688,0,970752,28409856,65551,28737536, 8 )
	
	//サポート：一歩追いかける感じで
	_OBJ_ANIME( C04_SUPPORT_01, anm_papa_support_move_02 )
	_OBJ_ANIME_WAIT()
	
	_CAMERA_WAIT_MOVE()	//カメラ移動待ち
	
	//…みつけたら ちゃんと帰るから！
	_BALLOONWIN_OBJMSG_POS( msg_c04_white_12, C04_SUPPORT_01, WIN_NONE )
	_MSGWIN_CLOSE()
	
	_TIME_WAIT( 30 )
	
	//親って大変なのね
	_BALLOONWIN_OBJMSG_POS( msg_c04_leader_05, MMDL_ID_EVENTOBJ_00, WIN_NONE )
	_MSGWIN_CLOSE()	
	
	//カミツレ：サポートを見て
	_OBJ_ANIME( MMDL_ID_EVENTOBJ_00, anm_stay_walk_l )
	_OBJ_ANIME_WAIT()
	//サポート：カミツレを見る
	_OBJ_ANIME( C04_SUPPORT_01, anm_stay_walk_r )
	_OBJ_ANIME_WAIT()
	
	//おせっかいだったかな
	_BALLOONWIN_OBJMSG_POS( msg_c04_leader_06, MMDL_ID_EVENTOBJ_00, WIN_NONE )
	_MSGWIN_CLOSE()	
	
	//バッジ取得で分岐
	_GET_BADGE_FLAG( SCWK_TEMP0, BADGE_ID_04 )	//バッジフラグ取得
	IF $SCWK_TEMP0 == 0 THEN
		//バッジ未入手なら
		
		//ジムで待ってるわ
		_BALLOONWIN_OBJMSG_POS( msg_c04_leader_07, MMDL_ID_EVENTOBJ_00, WIN_NONE )
		_MSGWIN_CLOSE()	
		
		//カミツレ：ジム側へ去る
		_OBJ_ANIME( MMDL_ID_EVENTOBJ_00, anm_papa_leader4_goaway_01a )
		_TIME_WAIT( 16 )
		_OBJ_ANIME( C04_SUPPORT_01, anm_stay_walk_d )
		_OBJ_ANIME_WAIT()
		
		
	ELSE
		//入手済みなら
		
		//カミツレ：主人公を見る
		_OBJ_ANIME( MMDL_ID_EVENTOBJ_00, anm_stay_walk_u )
		_OBJ_ANIME_WAIT()
		
		//５番道路で待ってるわ
		_PLAYER_NAME( 0 )
		_BALLOONWIN_OBJMSG_POS( msg_c04_leader_08, MMDL_ID_EVENTOBJ_00, WIN_NONE )
		_MSGWIN_CLOSE()	
		
		//サポート：道を開ける
		_OBJ_ANIME( C04_SUPPORT_01, anm_papa_support_move_03 )
		_TIME_WAIT( 8 )
		//カミツレ：５番道路側へ去る
		_OBJ_ANIME( MMDL_ID_EVENTOBJ_00, anm_papa_leader4_goaway_01b )
		//_OBJ_ANIME( MMDL_ID_PLAYER, anm_stay_walk_l )	//【保留】動きすぎなのでカット
		_OBJ_ANIME_WAIT()
		
		
	ENDIF
	
	//カミツレ削除
	_OBJ_DEL( MMDL_ID_EVENTOBJ_00 )
	
	_CAMERA_DEF_MOVE( 20 )		//カメラ主人公位置に復帰
	_CAMERA_WAIT_MOVE()	//カメラ移動待ち
	_CAMERA_BIND()		//カメラバインド
	_CAMERA_END()		//カメラ操作終了
	
	//バッジ取得で分岐
	_GET_BADGE_FLAG( SCWK_TEMP0, BADGE_ID_04 )	//バッジフラグ取得
	IF $SCWK_TEMP0 == 0 THEN
		//バッジ未入手なら
		
		//サポート：主人公を見て
		_OBJ_ANIME( C04_SUPPORT_01, anm_papa_support_move_04 )
		_OBJ_ANIME_WAIT()
		
	ELSE
		//入手済みなら
		
		//サポート：主人公を見て
		_OBJ_ANIME( C04_SUPPORT_01, anm_stay_walk_u )
		_OBJ_ANIME_WAIT()
		
		//主人公：サポートを見る 【保留】動かしすぎな気がする サポートとカミツレに注目してほしいのでカット
		//_OBJ_ANIME( MMDL_ID_PLAYER, anm_stay_walk_d )
		//_OBJ_ANIME_WAIT()
		
	ENDIF
	
	//かっこいい！
	_BALLOONWIN_OBJMSG_POS( msg_c04_white_13, C04_SUPPORT_01, WIN_NONE )
	_MSGWIN_CLOSE()
	
	//サポート：去る
	_OBJ_ANIME( C04_SUPPORT_01, anm_papa_support_goaway_01 )
	_OBJ_ANIME_WAIT()
	_OBJ_DEL( C04_SUPPORT_01 )
	
	
	//========================================//
	//シーンチェンジイベント無効
	$WK_SCENE_C04 = 3
	
	//サポート＆パパ非表示
	_FLAG_SET( FV_C04_SUPPORT_01 )
	_FLAG_SET( FV_C04_SUPPORTPAPA_01 )
	
	//========================================//

EVENT_END

