/************************************************************/
/*			サポートのミュージカル案内 posイベント			*/
/************************************************************/
EVENT_START	ev_pos_c04_musicalguide_01
	
	{
	
	DEFINE_LOCAL	LWK_POSX
	DEFINE_LOCAL	LWK_POSZ
	
	//おーい！
	_PLAYER_NAME( 0 )
	_PLAINWIN_MSG_DOWN( msg_c04_white_04 )
	_MSGWIN_CLOSE()
	
	//サポート曲再生
	_START_EVENT_BGM( SEQ_BGM_E_SUPPORT )
	
	//主人公：右を向く※ウェイトはサポート登場アニメと同じタイミング
	_OBJ_ANIME( MMDL_ID_PLAYER, anm_stay_walk_r )
	
	//主人公位置を取得
	_PLAYER_POS_GET( LWK_POSX,LWK_POSZ  )
	//サポートを主人公と同じZ軸に表示（436の場合は生垣に入ってしまうので437に出現）
	IF $LWK_POSZ == 436 THEN
		_OBJ_POS_CHANGE( C04_SUPPORT_01, 399, 0, 437, DIR_LEFT )
	ELSE
		_OBJ_POS_CHANGE( C04_SUPPORT_01, 399, 0, LWK_POSZ, DIR_LEFT )
	ENDIF
	
	//サポート：主人公横へ
	IF $LWK_POSZ == 436 THEN
		_OBJ_ANIME( C04_SUPPORT_01, anm_pos_support_come_01a )
	ELSE
		_OBJ_ANIME( C04_SUPPORT_01, anm_pos_support_come_01b )
	ENDIF
	_OBJ_ANIME_WAIT()
	
	//〜〜もミュージカルいこうよ
	_BALLOONWIN_OBJMSG_POS( msg_c04_white_05, C04_SUPPORT_01, WIN_NONE )
	_MSGWIN_CLOSE()
	
	//サポート：移動
	IF $LWK_POSZ == 436 THEN
		_OBJ_ANIME( C04_SUPPORT_01, anm_pos_support_move_01a )
		_OBJ_ANIME( MMDL_ID_PLAYER, anm_pos_player_move_01a )
	ELSIF $LWK_POSZ == 437 THEN
		_OBJ_ANIME( C04_SUPPORT_01, anm_pos_support_move_01b )
		_OBJ_ANIME( MMDL_ID_PLAYER, anm_pos_player_move_01b )
	ELSIF $LWK_POSZ == 438 THEN
		_OBJ_ANIME( C04_SUPPORT_01, anm_pos_support_move_01c )
		_OBJ_ANIME( MMDL_ID_PLAYER, anm_pos_player_move_01c )
	ELSIF $LWK_POSZ == 439 THEN
		_OBJ_ANIME( C04_SUPPORT_01, anm_pos_support_move_01d )
		_OBJ_ANIME( MMDL_ID_PLAYER, anm_pos_player_move_01d )
	ELSE
		_OBJ_ANIME( C04_SUPPORT_01, anm_pos_support_move_01e )
		_OBJ_ANIME( MMDL_ID_PLAYER, anm_pos_player_move_01e )
	ENDIF
	_OBJ_ANIME_WAIT()
	
	//はい到着
	_BALLOONWIN_OBJMSG_POS( msg_c04_white_06, C04_SUPPORT_01, WIN_NONE )
	_MSGWIN_CLOSE()
	
	//ミュージカル施設のマップ遷移処理へ
	_CALL	ev_map_change_musical
	
	}
	
EVENT_END

