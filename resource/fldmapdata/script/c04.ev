//ミュージカル施設のマップ遷移
ev_map_change_musical:

	//主人公＆サポート：ミュージカル内部へ
	_OBJ_ANIME( C04_SUPPORT_01, anm_music_support_move_01 )
	_TIME_WAIT( 12 )
	
	_PLAYER_DIR_GET( SCWK_ANSWER )	//主人公の向きで分岐
	IF $SCWK_ANSWER == DIR_UP THEN
		_OBJ_ANIME( MMDL_ID_PLAYER, anm_music_player_move_01u )
	ELSIF $SCWK_ANSWER == DIR_LEFT THEN
		_OBJ_ANIME( MMDL_ID_PLAYER, anm_music_player_move_01l )
	ELSE
		_OBJ_ANIME( MMDL_ID_PLAYER, anm_music_player_move_01r )
	ENDIF
	_OBJ_ANIME_WAIT()
	
	_CAMERA_START()		//カメラ操作開始
	_CAMERA_PURGE()		//カメラパージ
	//カメラ移動：ミュージカル施設全体を見せる
	_CAMERA_MOVE( 8664,0,1069056,28409856,151567,28246016, 20 )
	_CAMERA_WAIT_MOVE()	//カメラ移動待ち
	_CAMERA_END()		//カメラ操作終了
	
	//主人公：一歩前へ
	_OBJ_ANIME( MMDL_ID_PLAYER, anm_music_player_move_02 )
	_OBJ_ANIME_WAIT()
	
	//ミュージカル内部へマップ遷移
	_MAP_CHANGE( ZONE_ID_C04R0201, 14, 20, DIR_UP )

	_RET

