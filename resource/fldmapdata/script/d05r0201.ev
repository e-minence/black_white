/**************************************************************/
/*【仮】クリア後イベント：七賢人イベント                     */
/**************************************************************/
EVENT_START   ev_d05r0201_sage_02
	
	//プレイヤーネームを格納
	_PLAYER_NAME(0)

_TALK_OBJ_START()
DEFINE_LOCAL	LWK_DIR	

	//七賢人７・セリフ１
	_BALLOONWIN_TALKOBJ_MSG( msg_d05r0201_sage1_end_01 )
	_MSGWIN_CLOSE()
	
	//つめとぎ入手
	_ITEM_EVENT_KEYWAIT( ITEM_WAZAMASIN01, 1 )
	
	//七賢人７・セリフ２
	_BALLOONWIN_TALKOBJ_MSG( msg_d05r0201_sage1_end_02 )
	_MSGWIN_CLOSE()
	
	//階段SE
	_SE_PLAY( SEQ_SE_KAIDAN )
	_SE_WAIT()

	//ハンサム出現
	_OBJ_ADD_EX( 4,17,DIR_UP,MMDL_ID_EVENTOBJ_00,ICPO,MV_DMY )
	
	//ハンサム寄ってくる
	_PLAYER_DIR_GET( LWK_DIR )
	SWITCH $LWK_DIR
	CASE DIR_LEFT THEN
		_OBJ_ANIME( MMDL_ID_EVENTOBJ_00, anm_left_come )
		_OBJ_ANIME( D05R0201_SAGE_02, anm_stay_u2 )
		_OBJ_ANIME_WAIT()
	CASE DIR_RIGHT THEN
		_OBJ_ANIME( MMDL_ID_EVENTOBJ_00, anm_right_come )
		_OBJ_ANIME( D05R0201_SAGE_02, anm_stay_u2 )
		_OBJ_ANIME_WAIT()
	CASE DIR_UP THEN
		_OBJ_ANIME( MMDL_ID_EVENTOBJ_00, anm_up_come )
		_OBJ_ANIME( D05R0201_SAGE_02, anm_stay_r2 )
		_OBJ_ANIME_WAIT()
	CASE DIR_DOWN THEN
		_OBJ_ANIME( MMDL_ID_EVENTOBJ_00, anm_down_come )
		_OBJ_ANIME( D05R0201_SAGE_02, anm_stay_r2 )
		_OBJ_ANIME_WAIT()
	ENDSWITCH

	//ハンサムセリフ
	_BALLOONWIN_OBJMSG( msg_d05r0201_icpo_end_01 , MMDL_ID_EVENTOBJ_00 )
	_MSGWIN_CLOSE()

	//七賢人７・セリフ３
	_BALLOONWIN_TALKOBJ_MSG( msg_d05r0201_sage1_end_03 )
	_MSGWIN_CLOSE()

	SWITCH $LWK_DIR
	CASE DIR_LEFT THEN
		_OBJ_ANIME( MMDL_ID_EVENTOBJ_00, anm_left_come2 )
		_OBJ_ANIME( MMDL_ID_PLAYER, anm_stay_u )
		_OBJ_ANIME_WAIT()
	CASE DIR_RIGHT THEN
		_OBJ_ANIME( MMDL_ID_EVENTOBJ_00, anm_right_come2 )
		_OBJ_ANIME( MMDL_ID_PLAYER, anm_stay_u )
		_OBJ_ANIME_WAIT()
	CASE DIR_UP THEN
		_OBJ_ANIME( MMDL_ID_EVENTOBJ_00, anm_up_come2 )
		_OBJ_ANIME( MMDL_ID_PLAYER, anm_stay_r )
		_OBJ_ANIME_WAIT()
	CASE DIR_DOWN THEN
		_OBJ_ANIME( MMDL_ID_EVENTOBJ_00, anm_down_come2 )
		_OBJ_ANIME( MMDL_ID_PLAYER, anm_stay_r )
		_OBJ_ANIME_WAIT()
	ENDSWITCH

	//【仮】全部の七賢人を捕まえている場合
	IF $LOCALWORK10 == 7 THEN
		
		//ハンサムセリフ
		_BALLOONWIN_OBJMSG( msg_d05r0201_icpo_end_03 , MMDL_ID_EVENTOBJ_00 )
		_MSGWIN_CLOSE()
	
	//まだ捕まえていない場合
	ELSE
	
		//ハンサムセリフ
		_BALLOONWIN_OBJMSG( msg_d05r0201_icpo_end_02 , MMDL_ID_EVENTOBJ_00 )
		_MSGWIN_CLOSE()
	
	ENDIF
	
	//画面暗転
	_BLACK_OUT()
	_DISP_FADE_END_CHECK()

	_OBJ_DEL( MMDL_ID_EVENTOBJ_00 )
	_OBJ_DEL( D05R0201_SAGE_02 )
	
	_BLACK_IN()
	_DISP_FADE_END_CHECK()

	//========================================/
	
	//バニシュフラグセット
	_FLAG_SET( FV_D05R0201_SAGE7 )
	
	//========================================/

EVENT_END

