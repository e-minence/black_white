/*   R01　ポケモン捕獲デモ                                    */
/**************************************************************/

EVENT_START   ev_r01_poke_getdemo_01


	//【仮】専用曲鳴らす
	_START_EVENT_BGM( SEQ_BGM_E_DOCTOR )
	
	//プレイヤーネームを格納
	_PLAYER_NAME(0)
	
	_OBJ_ANIME( R01_RIVAL_01,anm_R01_pokeget_rival_look_player )
	_OBJ_ANIME( R01_SUPPORT_01,anm_R01_pokeget_rival_look_player )
	_OBJ_ANIME_WAIT()
	
	//サポート：おうい！（プレイヤー名）こっちよ！
	_PLAINWIN_MSG_DOWN( msg_r01_white_00)
	_PLAINWIN_CLOSE()
	
//POSイベント、POSごとに主人公移動アニメ
  { //ここからローカル変数の参照制限範囲開始
    DEFINE_LOCAL  POSX   /* ローカル変数定義 */
    DEFINE_LOCAL  POSZ    /* {}で囲う場合は連続して書かなければならない */

    _PLAYER_POS_GET( POSX,POSZ ) //自機座標取得
    SWITCH $POSX
    CASE 786 THEN
        _OBJ_ANIME( MMDL_ID_PLAYER,anm_r01_guide_x786 )
        _OBJ_ANIME_WAIT()
    CASE 787 THEN
        _OBJ_ANIME( MMDL_ID_PLAYER,anm_r01_guide_x787 )
        _OBJ_ANIME_WAIT()
    CASE 788 THEN  //目の前に立ったケース
        //なし
        _OBJ_ANIME( R01_RIVAL_01,anm_R01_pokeget_rival_look_player )
        _OBJ_ANIME_WAIT()
    CASE 789 THEN
        _OBJ_ANIME( MMDL_ID_PLAYER,anm_r01_guide_x789 )
        _OBJ_ANIME_WAIT()
    CASE 790 THEN
        _OBJ_ANIME( MMDL_ID_PLAYER,anm_r01_guide_x790 )
        _OBJ_ANIME_WAIT()
    CASE 791 THEN
        _OBJ_ANIME( MMDL_ID_PLAYER,anm_r01_guide_x791 )
        _OBJ_ANIME_WAIT()
    DEFAULT  //目の前に立ったケースと同じ処理を行う
        //なし
        _OBJ_ANIME_WAIT()
    ENDSWITCH
  } //ローカル変数の参照制限範囲、ここまで。

//上に移動
    _OBJ_ANIME( MMDL_ID_PLAYER,anm_r01_guide_up )
    _OBJ_ANIME_WAIT()

//ライバル、アニメ＋メッセージ
	_OBJ_ANIME( R01_RIVAL_01,anm_R01_pokeget_rival_look_hakase )
	_OBJ_ANIME( R01_SUPPORT_01,anm_R01_pokeget_support_look_hakase )
	_OBJ_ANIME_WAIT()
	_BALLOONWIN_OBJMSG_POS( msg_r01_black_01,R01_RIVAL_01 , POS_DL )
	_BALLOONWIN_CLOSE()

//博士、メッセージ＋アニメ
	_BALLOONWIN_OBJMSG_POS( msg_r01_hakase2_01,R01_HAKASE2_01 , POS_DL )
	_BALLOONWIN_CLOSE()
	_OBJ_ANIME( R01_HAKASE2_01,anm_R01_pokeget1 )
	
	//サポート＋ライバル博士を見る
	_OBJ_ANIME( R01_RIVAL_01,anm_R01_pokeget_up )
	_OBJ_ANIME( R01_SUPPORT_01,anm_R01_pokeget_up )
	_OBJ_ANIME( MMDL_ID_PLAYER,anm_R01_pokeget_up )
	
	_CAMERA_START()		//カメラ操作開始
	_CAMERA_PURGE()		//カメラのバインドを解く
	
	//カメラ操作　博士中心に
	_CAMERA_MOVE( 9688,0,970752,51675136,0,47337472,30 )
	_CAMERA_WAIT_MOVE()	//カメラ移動待ち
	
	_OBJ_ANIME_WAIT()

	//捕獲デモ呼び出し[100216斉藤マ]
	_CAPTURE_DEMO_SET()
	
	_CAMERA_START()		//カメラ操作開始
	_CAMERA_PURGE()		//カメラのバインドを解く
	
	//カメラ操作　博士中心に
	_CAMERA_MOVE( 9688,0,970752,51675136,0,47337472,1 )
	_CAMERA_WAIT_MOVE()	//カメラ移動待ち
	
	//暗転終了
	_WILD_WIN()
	
	//博士、メッセージ＋退場アニメ
	_OBJ_ANIME( R01_HAKASE2_01,anm_R01_pokeget1a )
	
	//自機にカメラを戻す
	_CAMERA_DEF_MOVE (30) 
	_CAMERA_WAIT_MOVE()
	_CAMERA_BIND()		//カメラバインド　主人公位置をカメラが追従するようになります
	_CAMERA_END()		//カメラ操作終了
	
	_OBJ_ANIME_WAIT()
	
	//サポート＋ライバル戻ってきた博士を見る
	_OBJ_ANIME( R01_RIVAL_01,anm_R01_pokeget_right )
	_OBJ_ANIME( R01_SUPPORT_01,anm_R01_pokeget_left )
	_OBJ_ANIME_WAIT()

	_BALLOONWIN_OBJMSG_POS( msg_r01_hakase2_02,R01_HAKASE2_01 , POS_DL )
	_BALLOONWIN_CLOSE()

//モンスターボール入手
	_ITEM_EVENT_KEYWAIT(ITEM_MONSUTAABOORU, 5)

	_BALLOONWIN_OBJMSG_POS( msg_r01_hakase2_03,R01_HAKASE2_01 , POS_DL )
	_BALLOONWIN_CLOSE()

	_OBJ_ANIME( R01_HAKASE2_01,anm_R01_pokeget2 )
	_OBJ_ANIME_WAIT()
	//専用曲止まる
	_END_EVENT_BGM()
	_OBJ_DEL( R01_HAKASE2_01 )

//ライバル、アニメ＋メッセージ
	_OBJ_ANIME( R01_RIVAL_01,anm_R01_pokeget3 )
	_OBJ_ANIME_WAIT()
	_BALLOONWIN_OBJMSG_POS( msg_r01_black_02,R01_RIVAL_01 , POS_UL )
	_BALLOONWIN_CLOSE()

	_OBJ_ANIME( R01_RIVAL_01,anm_R01_pokeget4 )
	_OBJ_ANIME_WAIT()
	_BALLOONWIN_OBJMSG_POS( msg_r01_black_03,R01_RIVAL_01 , POS_UL )
	_BALLOONWIN_CLOSE()

//サポート、アニメ＋メッセージ
	_BALLOONWIN_OBJMSG_POS( msg_r01_white_01,R01_SUPPORT_01 , POS_UR )
	_BALLOONWIN_CLOSE()

	_OBJ_ANIME( R01_SUPPORT_01,anm_R01_pokeget5 )
	_OBJ_ANIME_WAIT()

	_BALLOONWIN_OBJMSG_POS( msg_r01_white_02,R01_SUPPORT_01 , POS_UR )
	_BALLOONWIN_CLOSE()

//ライバル
	_BALLOONWIN_OBJMSG_POS( msg_r01_black_04,R01_RIVAL_01 , POS_UL )
	_BALLOONWIN_CLOSE()

//サポート
	_BALLOONWIN_OBJMSG_POS( msg_r01_white_03,R01_SUPPORT_01 , POS_UR )
	_BALLOONWIN_CLOSE()

//ライバル
	_OBJ_ANIME( R01_RIVAL_01,anm_R01_pokeget4_2 )
	_OBJ_ANIME_WAIT()
	_BALLOONWIN_OBJMSG_POS( msg_r01_black_05,R01_RIVAL_01 , POS_UL )
	_BALLOONWIN_CLOSE()

//サポート
	_BALLOONWIN_OBJMSG_POS( msg_r01_white_04,R01_SUPPORT_01 , POS_UR )
	_BALLOONWIN_CLOSE()

//ライバル、サポート退場アニメ
	_OBJ_ANIME( R01_RIVAL_01,anm_R01_pokeget6 )
	_OBJ_ANIME( R01_SUPPORT_01,anm_R01_pokeget7 )
	_OBJ_ANIME_WAIT()

	_OBJ_DEL( R01_RIVAL_01 )
	_OBJ_DEL( R01_SUPPORT_01 )

//==================================//

	//博士＆ライバル＆サポートを非表示
	_FLAG_SET( FV_R01_HAKASE )
	_FLAG_SET( FV_R01_RIVAL )
	_FLAG_SET( FV_R01_SUPPORT )

	//捕獲デモのpos無効。捕獲回数対決イベントのpos有効
	$WK_SCENE_R01 = 1

//==================================//



EVENT_END

