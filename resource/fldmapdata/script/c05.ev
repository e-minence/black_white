/*POSイベント：サポート登場                            */
/**************************************************************/
EVENT_START   ev_pos_c05_support_01

	//プレイヤーネームを格納
	_PLAYER_NAME(0)
	
	//サポート：しっぽなしメッセージ表示
	_PLAINWIN_MSG_DOWN( msg_c05_white_01 )
	_MSGWIN_CLOSE()
	
	//主人公「！」
	_OBJ_ANIME( MMDL_ID_PLAYER,anm_c05_player_gyoe )
	_OBJ_ANIME_WAIT()
	
	//サポート表示
	_OBJ_ADD_EX( 181, 414, DIR_UP, MMDL_ID_EVENTOBJ_00, SUPPORT, MV_DMY )
	
	//サポート曲再生
	_START_EVENT_BGM( SEQ_BGM_E_SUPPORT )
	
	//サポート主人公に隣接
	_OBJ_ANIME( MMDL_ID_EVENTOBJ_00, anm_c05_support_hello )
	_OBJ_ANIME_WAIT()

	//自機座標取得変数定義
	DEFINE_LOCAL  POSX
	DEFINE_LOCAL  POSZ

	_PLAYER_POS_GET( POSX,POSZ ) //自機座標取得
	
	//主人公座標に応じてサポートに隣接
	SWITCH $POSZ
	CASE 404 THEN
		_OBJ_ANIME( MMDL_ID_PLAYER,anm_c05_player_to_supportz404 )
		_OBJ_ANIME_WAIT()
	CASE 405 THEN
		_OBJ_ANIME( MMDL_ID_PLAYER,anm_c05_player_to_supportz405 )
	_OBJ_ANIME_WAIT()
	CASE 406 THEN  //目の前に立った時
		_OBJ_ANIME( MMDL_ID_PLAYER,anm_c05_player_to_supportz406 )
		 _OBJ_ANIME_WAIT()
	CASE 407 THEN
		_OBJ_ANIME( MMDL_ID_PLAYER,anm_c05_player_to_supportz407 )
		_OBJ_ANIME_WAIT()
	DEFAULT  //目の前に立ったケースと同じ処理を行う
		_OBJ_ANIME( MMDL_ID_PLAYER,anm_c05_player_to_supportz405 )
		_OBJ_ANIME_WAIT()
	ENDSWITCH
	
	//サポート：ねえねえ！ひょっとして
	_BALLOONWIN_OBJMSG(msg_c05_white_02,MMDL_ID_EVENTOBJ_00)
	_BALLOONWIN_CLOSE()
	
	//「！」マーク表示
	_OBJ_ANIME( MMDL_ID_EVENTOBJ_00, anm_c05_support_gyoe )
	_OBJ_ANIME_WAIT()
	
	//サポート：ダメダメ！
	_BALLOONWIN_OBJMSG(msg_c05_white_03,MMDL_ID_EVENTOBJ_00)
	_BALLOONWIN_CLOSE()
	
	//サポートと戦闘（持ちタイプで分岐）
	
	IF $WK_SYS_FIRST_POKETYPE == FIRST_POKETYPE_KUSA THEN
		//最初の３匹で草タイプを選択した時の処理
		_TRAINER_BTL_SET( TRID_SUPPORT_04, 0 )
		//結果チェック
		_TRAINER_LOSE_CHECK( SCWK_ANSWER )
		IF $SCWK_ANSWER == SCR_BATTLE_RESULT_WIN THEN
			_TRAINER_WIN()
		ELSE
			_TRAINER_LOSE()
		ENDIF

	ELSIF $WK_SYS_FIRST_POKETYPE == FIRST_POKETYPE_HONOO THEN	
		//最初の３匹で炎タイプを選択した時の処理
		_TRAINER_BTL_SET( TRID_SUPPORT_05, 0 )
		//結果チェック
		_TRAINER_LOSE_CHECK( SCWK_ANSWER )
		IF $SCWK_ANSWER == SCR_BATTLE_RESULT_WIN THEN
			_TRAINER_WIN()
		ELSE
			_TRAINER_LOSE()
		ENDIF
		
	ELSE
		//最初の３匹で水タイプを選択した時の処理
		_TRAINER_BTL_SET( TRID_SUPPORT_06, 0 )
		//結果チェック
		_TRAINER_LOSE_CHECK( SCWK_ANSWER )
		IF $SCWK_ANSWER == SCR_BATTLE_RESULT_WIN THEN
			_TRAINER_WIN()
		ELSE
			_TRAINER_LOSE()
		ENDIF
		
	ENDIF
	
	//サポート：ひさびさのポケモン勝負だったけど
	_BALLOONWIN_OBJMSG(msg_c05_white_04,MMDL_ID_EVENTOBJ_00)
	_BALLOONWIN_CLOSE()
	
	//退場アニメ（１）
	_OBJ_ANIME( MMDL_ID_EVENTOBJ_00, anm_c05_support_goodbye01 )
	_OBJ_ANIME_WAIT()
	
	_OBJ_ANIME( MMDL_ID_PLAYER,anm_c05_player_look_support_down )
	_OBJ_ANIME_WAIT()
	
	//サポート：あのね！
	_BALLOONWIN_OBJMSG(msg_c05_white_05,MMDL_ID_EVENTOBJ_00)
	_BALLOONWIN_CLOSE()
	
	//退場アニメ（２）
	_OBJ_ANIME( MMDL_ID_EVENTOBJ_00, anm_c05_support_goodbye02 )
	_OBJ_ANIME_WAIT()
	
	//サポートOBJ消す
	_OBJ_DEL( MMDL_ID_EVENTOBJ_00 )
	//サポート曲停止
	_END_EVENT_BGM()

	//============フラグワーク操作=============//
	
	//フラグ操作：C05のPOSをすべて無効に
	$WK_SCENE_C05 = 5

	//========================================//

EVENT_END

