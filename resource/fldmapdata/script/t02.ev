EVENT_START   ev_t02_hakase_guide
	//まちかねたぞ！
	_TALK_OBJ_START()
	_BALLOONWIN_TALKOBJ_OPEN( msg_t02_doctor_jr_01 )
	_AB_KEYWAIT()
	_BALLOONWIN_CLOSE()
		
	//ポケモンセンターに消えていく
	_OBJ_ANIME( T02_HAKASE_GUIDE, anm_doctor_pokecen )
	_OBJ_ANIME_WAIT()

	//博士の動作モデルを削除する
	_OBJ_DEL_EV( T02_HAKASE_GUIDE )

	//自機も一緒についていく
	_OBJ_ANIME( MMDL_ID_PLAYER, anm_player_pokecen )
	_OBJ_ANIME_WAIT()
	
	//ゾーンチェンジ
	_MAP_CHANGE( ZONE_ID_T02PC0101, 7, 19, DIR_UP )

	//博士案内イベント終了
	_FLAG_SET( FV_T02_HAKASE )
	
EVENT_END

