/*	【仮】七賢者ゲーチス　演説　細かい演出は未調整			*/
/************************************************************/
EVENT_START   ev_pos_t02_rival_speech
	//友達♂：気付く
	_OBJ_ANIME( T02_RIVAL_02 , anm_rival_kiduku1 )
	_OBJ_ANIME_WAIT()
	
	//ローカル変数を定義
	DEFINE_LOCAL	SPEECH_X
	DEFINE_LOCAL	SPEECH_Z

	//友達♂：近づく
	_PLAYER_POS_GET( SPEECH_X,SPEECH_Z )		//主人公位置取得
	IF $SPEECH_Z == 655 THEN
		//Z軸が655なら
		_OBJ_ANIME( T02_RIVAL_02, anm_rival_tikaduku655 )
		_OBJ_ANIME( MMDL_ID_PLAYER, anm_player_miru_d )
	ELSE
		//Z軸が656なら
		_OBJ_ANIME( T02_RIVAL_02, anm_rival_tikaduku656 )
	ENDIF
	_OBJ_ANIME_WAIT()
	//おい　なんだか　おもしろいことが
	_BALLOONWIN_OBJMSG_OPEN( msg_t02_black_02 , T02_RIVAL_02 )
	_BALLOONWIN_CLOSE()

	//友達♂：誘導
	DEFINE_LOCAL	SPEECH1_X
	DEFINE_LOCAL	SPEECH1_Z

	//友達♂：近づく
	_PLAYER_POS_GET( SPEECH1_X,SPEECH1_Z )		//主人公位置取得
	IF $SPEECH1_Z == 655 THEN
		//Z軸が655なら
		_OBJ_ANIME( T02_RIVAL_02, anm_rival_yudo655 )
		_OBJ_ANIME( MMDL_ID_PLAYER, anm_player_yudo655 )
	ELSE
		//Z軸が656なら
		_OBJ_ANIME( T02_RIVAL_02, anm_rival_yudo656 )
		_OBJ_ANIME( MMDL_ID_PLAYER, anm_player_yudo656 )
	ENDIF
	_OBJ_ANIME_WAIT()

	//わたくしの　なまえは　ゲーチス
	_BALLOONWIN_OBJMSG_OPEN( msg_t02_sage1_01 , T02_SAGE_01 )
	_AB_KEYWAIT()
	_BALLOONWIN_CLOSE()
	
	//群集：飛び跳ねる
	_OBJ_ANIME( T02_BOODLE_01, anm_boodle_jump )
	
	//群集「えっ？」
	_BALLOONWIN_OBJMSG_OPEN( msg_t02_boodle_01 , T02_BOODLE_01 )
	_AB_KEYWAIT()
	_BALLOONWIN_CLOSE()	
	_OBJ_ANIME_WAIT()

	//われわれ　人間は　ポケモンと　ともに
	//ゲーチス：左右に動く
	_BALLOONWIN_OBJMSG_OPEN( msg_t02_sage2_02 , T02_SAGE_01 )
	_OBJ_ANIME( T02_SAGE_01, anm_sage_move )
	_AB_KEYWAIT()
	_BALLOONWIN_CLOSE()

	
	//トレーナーは　ポケモンに　好き勝手　命令している
	_BALLOONWIN_OBJMSG_OPEN( msg_t02_sage3_03 , T02_SAGE_01 )
	_BALLOONWIN_CLOSE()
	_OBJ_ANIME_WAIT()
	_AB_KEYWAIT()
	
	//群集「ざわざわ」
	_OBJ_ANIME( T02_BOODLE_01, anm_boodle_jump )
	_BALLOONWIN_OBJMSG_OPEN( msg_t02_boodle_02 , T02_BOODLE_01 )
	_AB_KEYWAIT()
	_BALLOONWIN_CLOSE()
	_OBJ_ANIME_WAIT()

	//いいですか　みなさん　ポケモンは　人間とは　異なり
	_BALLOONWIN_OBJMSG_OPEN( msg_t02_sage4_04 , T02_SAGE_01 )
	_AB_KEYWAIT()
	_BALLOONWIN_CLOSE()
	
	//群集「？」
	_OBJ_ANIME( T02_BOODLE_01, anm_boodle_jump )
	_BALLOONWIN_OBJMSG_OPEN( msg_t02_boodle_03 , T02_BOODLE_01 )
	_AB_KEYWAIT()
	_BALLOONWIN_CLOSE()
	_OBJ_ANIME_WAIT()

	//そうです！　ポケモンを　解放することです！
	_BALLOONWIN_OBJMSG_OPEN( msg_t02_sage4_05 , T02_SAGE_01 )
	_AB_KEYWAIT()
	_BALLOONWIN_CLOSE()

	//ゲーチスを消す
	_FLAG_SET( FV_T02_SAGE )
	_OBJ_DEL( T02_SAGE_01 )
	
	//部下を消す
	_FLAG_SET( FV_T02_HAKAI )
	_OBJ_DEL( T02_HAKAIW_01 )
	_OBJ_DEL( T02_HAKAIM_01 )
	
	//群集おじいさん：キョロキョロする
	//今の　演説　わしたちは　どうすれば　いいんだ
	_OBJ_ANIME( T02_BOODLE_01, anm_boodle_jump )
	_BALLOONWIN_OBJMSG_OPEN( msg_t02_boodle_04 , T02_BOODLE_01 )
	_AB_KEYWAIT()
	_BALLOONWIN_CLOSE()
	_OBJ_ANIME_WAIT()
	
	//群集おにいさん：少し大きめにアクション、キョロキョロ、ジャンプ
	//ポケモンを　解放って　そんな話　ありえないでしょ！
	_OBJ_ANIME( T02_BOODLE_01, anm_boodle_jump )
	_BALLOONWIN_OBJMSG_OPEN( msg_t02_boodle_05 , T02_BOODLE_01 )
	_AB_KEYWAIT()
	_BALLOONWIN_CLOSE()
	_OBJ_ANIME_WAIT()

	//Ｎ出現
	_FLAG_RESET( FV_T02_NNN )
	_OBJ_ADD( T02_N_01 )

	//Ｎ：自機の真横に移動
	_OBJ_ANIME( T02_N_01, anm_n_tikaduku )
	_OBJ_ANIME_WAIT()
	
	//自機：Ｎの方を向く
	_OBJ_ANIME( MMDL_ID_PLAYER, anm_player_miru_r )
	_OBJ_ANIME_WAIT()

	//友達♂：Ｎの方を向く
	_OBJ_ANIME( T02_RIVAL_02, anm_rival_miru_r )
	_OBJ_ANIME_WAIT()

	//キミの　ポケモンかな？　今　トレーナーへの
	_BALLOONWIN_OBJMSG_OPEN( msg_t02_boss_01 , T02_N_01 )
	_AB_KEYWAIT()
	_BALLOONWIN_CLOSE()


	//友達♂：Ｎの前に移動、その場ジャンプ
	_OBJ_ANIME( T02_RIVAL_02, anm_rival_jump )
	_OBJ_ANIME_WAIT()

	//えっ！？　ポケモンが　話しているんですか？
	_BALLOONWIN_OBJMSG_OPEN( msg_t02_black_03 , T02_RIVAL_02 )
	_AB_KEYWAIT()
	_BALLOONWIN_CLOSE()

	//Ｎ：友達♂の方を向く
	
	//ああ　話しているよ　そうか　キミたちにも　聞こえないのか
	_BALLOONWIN_OBJMSG_OPEN( msg_t02_boss_02 , T02_N_01 )
	_AB_KEYWAIT()
	_BALLOONWIN_CLOSE()


	//オレは　クロックで　こちらは　自機　です
	_BALLOONWIN_OBJMSG_OPEN( msg_t02_black_04 , T02_RIVAL_02 )
	_AB_KEYWAIT()
	_BALLOONWIN_CLOSE()

	//ポケモン図鑑……！？　がっかりだな
	_BALLOONWIN_OBJMSG_OPEN( msg_t02_boss_03 , T02_N_01 )
	_AB_KEYWAIT()
	_BALLOONWIN_CLOSE()

	//【仮】Ｎとポケモンしょうぶ	バトル未実装
	
	//……負けたか　ボクの　友達たち　ありがとう
	_BALLOONWIN_OBJMSG_OPEN( msg_t02_boss_04 , T02_N_01 )
	_AB_KEYWAIT()
	_BALLOONWIN_CLOSE()

	//Ｎ：２番道路へ移動
	_OBJ_ANIME( T02_N_01, anm_n_bye )
	_OBJ_ANIME_WAIT()

	//【仮】友達♂：Ｎを目で追う
	
	//【仮】自機：Ｎを目で追う

	//Ｎを消す
	_OBJ_DEL( T02_N_01 )

	//【仮】友達♂：Ｎの方を見た後、自機を見る
	
	//なんか　オレのせいで　変な話に　なっちゃった
	_BALLOONWIN_OBJMSG_OPEN( msg_t02_black_05 , T02_RIVAL_02 )
	_AB_KEYWAIT()
	_BALLOONWIN_CLOSE()

	//友達♂：２番道路へ移動
	_OBJ_ANIME( T02_RIVAL_02, anm_rival_bye )
	_OBJ_ANIME_WAIT()

	//友達♂を消す
	_OBJ_DEL( T02_RIVAL_02 )

	//============================================//
	
	//Ｎと友達♂のバニッシュフラグセット
	_FLAG_SET( FV_T02_NNN )
	_FLAG_SET( FV_T02_RIVAL_02 )

	//============================================//
	
EVENT_END

