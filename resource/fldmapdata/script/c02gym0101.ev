/*     博物館受付嬢 1                　　　　　　　　　　　　　*/
/**************************************************************/
EVENT_START		 ev_c02gym0101_informationw_01
	
	//盗難イベント中？
	IF FLAG_ON( FE_MUSEUM_TOUNAN ) THEN
		//盗難イベントが発生中
		//いきなり　ポケモンで　おどろかされて……
		_EASY_BALLOONWIN_TALKOBJ_MSG  ( msg_c02gym0101_counter1_02)
	
	ELSE
		
		//盗難イベントではない
		//ポケモンジムへは
		_EASY_BALLOONWIN_TALKOBJ_MSG  ( msg_c02gym0101_counter1_01)
	
	ENDIF 

EVENT_END

/**************************************************************/
/*     博物館受付嬢 2                　　　　　　　　　　　　　*/
/**************************************************************/
