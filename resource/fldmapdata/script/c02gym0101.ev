/*     ジムリーダーアロエが博物館を出て行く					*/
/**************************************************************/
EVENT_START		ev_pos_c02gym0101_leader_01
	
	//アロエと、ジムリーダー夫を配置する
	_OBJ_ADD_EX( 12, 11, DIR_DOWN, MMDL_ID_EVENTOBJ_00, LEADER2, MV_DOWN )
	_OBJ_POS_CHANGE( C02GYM0101_AD_01, 14, 0, 13, DIR_DOWN )
	
	//主人公が、アロエの元まで歩く
	_OBJ_ANIME( MMDL_ID_PLAYER, anm_player_idu_tounan )
	_OBJ_ANIME_WAIT()
	
	//アロエが退場
	_OBJ_ANIME(MMDL_ID_EVENTOBJ_00, anm_leader_idou_tounan_a )
	_OBJ_ANIME_WAIT()
	
	//アロエが喋る
	//なんてこったい……
	_BALLOONWIN_OBJMSG( msg_c02gym0101_leader_01, MMDL_ID_EVENTOBJ_00 )
	_BALLOONWIN_CLOSE()

	//アロエが退場
	_OBJ_ANIME(MMDL_ID_EVENTOBJ_00, anm_leader_idou_tounan_b )
	_OBJ_ANIME_WAIT()
	
	//ジムリーダー：消す
	_OBJ_DEL( MMDL_ID_EVENTOBJ_00 )
	
	//==============フラグワーク操作==============//
		
		//アロエが博物館を飛び出すイベントを無効にする
		$WK_SCENE_C02GYM0101 = 3
		
	//============================================//
	
EVENT_END

