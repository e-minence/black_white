/**************************************************************/
/*     副館長												　*/
/**************************************************************/
//【仮】副館長のOBJコードは仮。イベント命名も仮です。
EVENT_START		ev_c02gym0101_ad_01
	
	//バッジを入手しているかどうか、チェック
	 _GET_BADGE_FLAG( SCWK_TEMP0, BADGE_ID_02 )
	
	////盗難イベント中？
	IF FLAG_ON( FE_MUSEUM_TOUNAN ) THEN
		//盗難イベントが発生中
		_EASY_BALLOONWIN_TALKOBJ_MSG  ( msg_c02gym0101_ad_07 ) 
	
	ELSE
		//盗難イベントではない
		//バッジを持ってる？
		IF $SCWK_TEMP0 == 0 THEN
			//持ってない
			//あの扉の　先が
			_EASY_BALLOONWIN_TALKOBJ_MSG  ( msg_c02gym0101_ad_06)
		
		ELSE
			//持ってる
			//骨格標本って　不思議なんですよ
			_EASY_BALLOONWIN_TALKOBJ_MSG  ( msg_c02gym0101_ad_08)
		
		ENDIF 
	ENDIF 

EVENT_END
/**************************************************************/
/*     副館長の博物館案内イベント							　*/
/**************************************************************/
EVENT_START		ev_pos_c02gym0101_ad_01

	//博物館案内イベント


		DEFINE_LOCAL  POSX   /* ローカル変数定義 */
		DEFINE_LOCAL  POSZ    /* {}で囲う場合は連続して書かなければならない */

		_PLAYER_POS_GET( POSX,POSZ ) //自機座標取得
    
		//うーむ！　この　骨格標本は　いつみても……　ホレボレしますな
		_BALLOONWIN_OBJMSG( msg_c02gym0101_ad_01, C02GYM0101_AD_01 )
		
		//副館長：発言を終えるまで、溜める（ホレボレしている）
		_TIME_WAIT  ( 8 ) 
		_OBJ_ANIME_WAIT()
		
		_BALLOONWIN_CLOSE()
		
		//副館長：！マーク
		_OBJ_ANIME( C02GYM0101_AD_01,anm_ad_bikkuri_mark)
		_OBJ_ANIME_WAIT()
		
		//副館長：主人公を向く
		_OBJ_ANIME( C02GYM0101_AD_01,anm_ad_shita_muku)
		_OBJ_ANIME_WAIT()
		
		//副館長：発言までに、ウェイトを持たせる
		_TIME_WAIT  ( 32 ) 
		_OBJ_ANIME_WAIT()
		
		//どうも　わたくし　副館長の　？？？　です　せっかく　いたしたのです〜
		_BALLOONWIN_OBJMSG( msg_c02gym0101_ad_02, C02GYM0101_AD_01 )
		_BALLOONWIN_CLOSE()
		
		//副館長：主人公の前まで来る
		//POSの3グリッド分を用意する
		SWITCH $POSX
		CASE 11 THEN
		_OBJ_ANIME( C02GYM0101_AD_01,anm_ad_shita_kuru_a)
		_OBJ_ANIME_WAIT()	

		CASE 12 THEN
		_OBJ_ANIME( C02GYM0101_AD_01,anm_ad_shita_kuru_b)
		_OBJ_ANIME_WAIT()	
		     
		CASE 13 THEN
		_OBJ_ANIME( C02GYM0101_AD_01,anm_ad_shita_kuru_c)
		_OBJ_ANIME_WAIT()	

		DEFAULT
		ENDSWITCH
		
		//副館長：主人公が動くまで、ウェイトを持たせる（主人公の躊躇の表現）
		_TIME_WAIT  ( 32 ) 
		_OBJ_ANIME_WAIT()
		
		//副館長：展示物の方を向く
		_OBJ_ANIME( C02GYM0101_AD_01,anm_ad_ue_muku)
		_OBJ_ANIME_WAIT()	
		
		//副館長：展示物（骨（大））まで移動
		//プレイヤーが通る3グリッド分を用意
		//主人公：展示物（骨（大））まで移動
		
		SWITCH $POSX
		CASE 11 THEN
		_OBJ_ANIME( C02GYM0101_AD_01,anm_ad_idou_hone_dai_a)
		_OBJ_ANIME( MMDL_ID_PLAYER,anm_player_idou_hone_dai_a)
		_OBJ_ANIME_WAIT()	
	
		CASE 12 THEN
		_OBJ_ANIME( C02GYM0101_AD_01,anm_ad_idou_hone_dai_b)
		_OBJ_ANIME( MMDL_ID_PLAYER,anm_player_idou_hone_dai_b)
		_OBJ_ANIME_WAIT()	
     
		CASE 13 THEN
		_OBJ_ANIME( C02GYM0101_AD_01,anm_ad_idou_hone_dai_c)
		_OBJ_ANIME( MMDL_ID_PLAYER,anm_player_idou_hone_dai_c)
		_OBJ_ANIME_WAIT()	
		
		DEFAULT
		ENDSWITCH
		
		//副館長：骨（大）の説明
		//骨格……　大昔の　ポケモン……
		_BALLOONWIN_OBJMSG( msg_c02gym0101_ad_03, C02GYM0101_AD_01 )
		_BALLOONWIN_CLOSE()
		
		//副館長：展示物（隕石）まで移動
		_OBJ_ANIME( C02GYM0101_AD_01,anm_ad_idou_inseki)	
		
		//主人公：展示物（隕石）まで移動
		_OBJ_ANIME( MMDL_ID_PLAYER,anm_player_idou_inseki)
		_OBJ_ANIME_WAIT()		
		
		//副館長：隕石の説明
		//この　石は　すごいですよ
		_BALLOONWIN_OBJMSG( msg_c02gym0101_ad_04, C02GYM0101_AD_01 )
		_BALLOONWIN_CLOSE()
		
		//副館長：展示物（石）まで移動
		_OBJ_ANIME( C02GYM0101_AD_01,anm_ad_idou_ishi)	
		
		//主人公：展示物（石）まで移動
		_OBJ_ANIME( MMDL_ID_PLAYER,anm_player_idou_ishi)
		_OBJ_ANIME_WAIT()		
		
		//副館長：石の説明
		//ああ　こちらは　ただの　ふるいいし　です
		_BALLOONWIN_OBJMSG( msg_c02gym0101_ad_05, C02GYM0101_AD_01 )
		_BALLOONWIN_CLOSE()
		
		//副館長：ジムまで移動
		_OBJ_ANIME( C02GYM0101_AD_01,anm_ad_idou_jimu)	
		
		//主人公：ジムまで移動
		_OBJ_ANIME( MMDL_ID_PLAYER,anm_player_idou_jimu)
		_OBJ_ANIME_WAIT()		
		
		//副館長：ジムの説明
		//あの扉の　先が　ポケモンジムと　なっております
		_BALLOONWIN_OBJMSG( msg_c02gym0101_ad_06, C02GYM0101_AD_01 )
		_BALLOONWIN_CLOSE()
		
		//==============フラグワーク操作==============//
		
		//博物館案内イベントを無効にする
		$WK_SCENE_C02GYM0101 = 1
		
		//============================================//
	
EVENT_END
