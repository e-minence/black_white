/************************************************************/
/*					FIELD_INIT_LABEL						*/
/************************************************************/
INIT_EVENT_START	ev_init_c01gym0101

	//分岐：ジムバッジ取得しているか、いないか
	_GET_BADGE_FLAG( SCWK_TEMP0, BADGE_ID_01 )
	
	//ジムバッジを取得していない場合
	IF $SCWK_TEMP0 == 0 THEN

		//ジムリーダー演出イベント後ならば
		IF $WK_SCENE_C01GYM0101 == 1 THEN

			//くさタイプ選択の場合
			IF $WK_SYS_FIRST_POKETYPE  == FIRST_POKETYPE_KUSA THEN
		
				//コーンの位置を変更する
				_OBJ_POS_CHANGE( C01GYM0101_LEADER1A_01, 14, 1, 3, DIR_DOWN )
				//ポッドの位置を変更する
				_OBJ_POS_CHANGE( C01GYM0101_LEADER1B_01, 12, 1, 4, DIR_DOWN )
				//デントの位置を変更する
				_OBJ_POS_CHANGE( C01GYM0101_LEADER1C_01, 10, 1, 3, DIR_DOWN )

			//ほのおタイプ選択の場合
			ELSIF $WK_SYS_FIRST_POKETYPE  == FIRST_POKETYPE_HONOO THEN

				//コーンの位置を変更する
				_OBJ_POS_CHANGE( C01GYM0101_LEADER1A_01, 12, 1, 4, DIR_DOWN )
				//ポッドの位置を変更する
				_OBJ_POS_CHANGE( C01GYM0101_LEADER1B_01, 10, 1, 3, DIR_DOWN )
				//デントの位置を変更する
				_OBJ_POS_CHANGE( C01GYM0101_LEADER1C_01, 14, 1, 3, DIR_DOWN )

			//みずタイプ選択の場合
			ELSE
		
				//コーンの位置を変更する
				_OBJ_POS_CHANGE( C01GYM0101_LEADER1A_01, 14, 1, 3, DIR_DOWN )
				//ポッドの位置を変更する
				_OBJ_POS_CHANGE( C01GYM0101_LEADER1B_01, 10, 1, 3, DIR_DOWN )
				//デントの位置を変更する
				_OBJ_POS_CHANGE( C01GYM0101_LEADER1C_01, 12, 1, 4, DIR_DOWN )

			ENDIF

		ELSE
		
		ENDIF

	//ジムバッジを取得している場合
	ELSE
	
		//コーンの位置を変更する
		_OBJ_POS_CHANGE( C01GYM0101_LEADER1A_01, 14, 1, 3, DIR_DOWN )
		//ポッドの位置を変更する
		_OBJ_POS_CHANGE( C01GYM0101_LEADER1B_01, 10, 1, 3, DIR_DOWN )
		//デントの位置を変更する
		_OBJ_POS_CHANGE( C01GYM0101_LEADER1C_01, 12, 1, 3, DIR_DOWN )
	
	ENDIF

INIT_EVENT_END

