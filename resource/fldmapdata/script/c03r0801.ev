/*		3号室のふたごちゃん片割れ１								  */
/**************************************************************/
EVENT_START		ev_c03r0801_babygirl1_01

//※トレーナーでない場合はバニッシュしてしまうので、
//この部屋のバトルタイプを調べることは、厳密にはいらない処理だが念のため

//ローカル変数を定義
	DEFINE_LOCAL	LWK_PAIR1_BTL_TYPE
  //ローカルワーク0に部屋番号を格納する
  $LOCALWORK0 = 2   //3号室
  //3号室のバトルタイプを取得
  _PL_BOAT_GET_ROOM_INFO(LOCALWORK0,PL_BOAT_ROOM_INFO_BTL_TYPE,0,LWK_PAIR1_BTL_TYPE)
  IF $LWK_PAIR1_BTL_TYPE == PL_BOAT_BTL_TYPE_DOUBLE THEN
    //ローカルワーク1に1を格納する（話しかけたのはダブルバトル相方）
    $LOCALWORK1 = PL_BOAT_TALK_PAIR
    _CALL ev_trainer
  ENDIF
EVENT_END

