    _FLAG_SET( FV_C03R0801_FUTAGO_01 )
  ENDIF

  _PL_BOAT_GET_ROOM_INFO(7,PL_BOAT_ROOM_INFO_BTL_TYPE,0,LWK_BTL_TYPE)
  IF $LWK_BTL_TYPE == PL_BOAT_BTL_TYPE_SINGLE THEN
    _FLAG_SET( FV_C03R0801_FUTAGO_02 )
  ENDIF

  _END

